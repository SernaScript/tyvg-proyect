{
  "functions": {
    "src/app/api/flypass-scraping/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/process-excel/route.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "0",
      "PLAYWRIGHT_BROWSERS_PATH": "/tmp/playwright",
      "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS": "true",
      "PLAYWRIGHT_SKIP_DEPENDENCY_INSTALLATION": "true",
      "PLAYWRIGHT_DOWNLOAD_HOST": "https://playwright.azureedge.net",
      "DEBIAN_FRONTEND": "noninteractive",
      "CI": "true"
    }
  },
  "outputDirectory": ".next",
  "installCommand": "npm install --production=false && npm run install-playwright-vercel",
  "buildCommand": "npm run build"
}
